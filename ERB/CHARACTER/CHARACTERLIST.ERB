@CHARACTER_LIST, ARG ;角色列表 1=显示能力
    #DIM CHARACTERNUM
    #DIM NUM
    NUM = 1
    PRINTL 角色列表
    CUSTOMDRAWLINE -
    PRINTFORML [1] %NAME:MASTER%
    FOR CHARACTERNUM, 2, 100
        NUM += 1
        IF EXISTCSV(CHARACTERNUM-1,0) == 1  
            PRINTFORML [{NUM}] %NAME:NUM%
        ENDIF
    NEXT
    PRINTFORML [99] 返回
    INPUT
    IF EXISTCSV(RESULT,0) == 1
        CALL CHARACTER_LIST_CHECK, RESULT ,ARG
    ENDIF

